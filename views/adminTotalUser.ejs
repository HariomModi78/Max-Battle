<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/boiler.css">
    <link rel="stylesheet" href="/stylesheets/adminPanel.css">
    <link rel="stylesheet" href="/stylesheets/adminTotalUser.css">
    <link rel="manifest" href="manifest.json">
    <meta name="theme-color" content="#007bff">
</head>
<body>
    <main>
        <div class="sideBar">
            <div class="esportText">Admin Panel</div>
            <div class="option">
                <img src="https://cdn-icons-png.flaticon.com/128/8323/8323511.png" class="home" alt="">
                <div class="optionName home">Dashboard</div>
            </div>
            <div class="option">
                <img src="https://cdn-icons-png.flaticon.com/128/9743/9743061.png" class="profile" alt="">
                <div class="optionName profile">Tournaments</div>
            </div>
            <div class="option">
                <img src="https://cdn-icons-png.flaticon.com/128/476/476863.png" class="login" alt="">
                <div class="optionName login">Users</div>
            </div>
            <div class="option">
                <img src="https://cdn-icons-png.flaticon.com/128/6811/6811049.png" class="myTournament" alt="">
                <div class="optionName myTournament">Pending Results</div>
            </div>
            <div class="option">
                <img src="https://cdn-icons-png.flaticon.com/128/7653/7653930.png" class="support" alt="">
                <div class="optionName support">Announcements</div>
            </div>
            <div class="option">
                <img src="https://cdn-icons-png.flaticon.com/128/4400/4400828.png" class="privacyPolicy" alt="">
                <div class="optionName privacyPolicy">Logout</div>
            </div>
        </div>
        <div class="page">
            <div class="topBar">
                <div class="mobileMenu">
                    <img class="mobileMenu" src="https://cdn-icons-png.flaticon.com/128/2099/2099125.png" alt="">
                </div>
                Total Users
            </div>
            
            <div class="searchBox">
                <input type="search" name="user" id="">
                <div class="searchIcon">
                    <img src="https://cdn-icons-png.flaticon.com/128/11310/11310503.png" alt="">
                </div>
                
            </div>
            
            <div class="events">
                <%users.forEach(function(val){%>
                    <div class="event">
                    <div class="eventPart1">
                        <img src="https://cdn-icons-png.flaticon.com/128/166/166258.png" alt="">
                    </div>
                    <div class="eventPart2">
                        <div class="eventDetail"><%=val.username%> <div class="gameName">(<%=val.gameName%>>)</div></div>
                        <div class="eventDetail">Balance : 
                            <img class="verySmallCoin" src="https://cdn-icons-png.flaticon.com/128/9382/9382295.png" alt="">
                            <%=val.totalBalance%></div>
                    </div>
                </div>
                <%})%>
                
                
                
            </div>
        </div>
    </main>

    <script src="/javaScripts/adminPanel.js"></script>
    <script src="/script.js"></script>
</body>
</html>